{
	"manifest_version": 2,
	"name": "Tabasco",
	"description": "Spicy Tab Managment",
	"version": "0.3.1",
	"offline_enabled": true,
	"homepage_url": "https://github.com/donatj/tabasco",
	"browser_action": {
		"default_icon": "icon128.png",
		"default_title": "Tabasco: Spicy Tab Managment",
		"default_popup": "index.html"
	},
	"permissions": [
		"activeTab",
		"tabs"
	]
}